import java.util.Scanner;
class Palindro
{
	public static void main(String[]args)
	{
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the number");
		int num = sc.nextInt();
		int num2 = num;
		int rev = 0;
		while (num2!=0)
		{
			int a = num2%10;
			rev = rev * 10 + a;
			num2/=10;
		}
			System.out.println(rev);

	}
}