import java.util.Scanner;
class NthFiboVal
{
	public static void main(String[]args)
	{
		Scanner sc =  new Scanner(System.in);
		System.out.println("Enter the value of n :");
		int n = sc.nextInt();
		
		int n1 = 0; n2 = 1; n3;

		for (int i = n1; i < n-1 ; i++ )
		{
			n3 = n1 + n2;
			n1 = n2;
			n2 = n3;
		}
		System.out.println("The "+n+"th Fibonacci value is "+n1);
	}
}